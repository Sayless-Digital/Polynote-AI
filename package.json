{
  "name": "polynote",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cd apps/web && npm run dev",
    "dev:turbo": "cd apps/web && next dev --turbo",
    "build": "cd apps/web && npm run build",
    "start": "cd apps/web && npm run start",
    "lint": "cd apps/web && npm run lint",
    "db:generate": "cd packages/db && npm run db:generate",
    "db:push": "cd packages/db && npm run db:push",
    "db:migrate": "cd packages/db && npm run db:migrate",
    "db:studio": "cd packages/db && npm run db:studio"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3"
  }
}
